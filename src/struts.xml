<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name="struts.custom.i18n.resources" value="messages" />
	<constant name="struts.ui.theme" value="simple" />
	<package name="rentcar" namespace="/Page" extends="struts-default">
		<action name="login" class="com.hp.easycar.action.Login">
			<result name="loginSuccess">/Page/welcome.jsp</result>
			<result name="loginError">/Page/error.jsp</result>
		</action>
		<action name="loginAction" class="com.hp.easycar.action.LoginAction">
			<result name="success">/Page/welcome.jsp</result>
			<result name="error">/Page/error.jsp</result>
		</action>
		<action name="loginActionSupport" class="com.hp.easycar.action.LoginActionSupport">
			<result name="input">/Page/loginTags.jsp</result>
			<result name="success">/Page/welcome.jsp</result>
			<result name="error">/Page/error.jsp</result>
		</action>
	</package>
</struts>    
